!--------------!
! URXVT CONFIG !
!--------------!

URxvt.internalBorder:	30
URxvt.scrollBar:	false
URxvt.cursorColor: #d3d0c8
urxvt.saveLines: 1000000
URxvt*font: xft:Ubuntu Mono:size=13
URxvt.letterSpace: 1

urxvt.perl-ext-common: default,matcher,font-size
urxvt.url-select.launcher: /usr/bin/google-chrome
urxvt.matcher.button: C1
URxvt.keysym.Control-0x2b: perl:font-size:increase
URxvt.keysym.Control-0x2d: perl:font-size:decrease
URxvt*scrollBar:          False
URxvt*depth:              32
URxvt*background:         [85]#1D1F21
URxvt*intensityStyles:    True
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.iso14755: false
URxvt.iso14755_52: falseâŽˆ
URxvt.keysym.Control-Up:     \033[1;5A
URxvt.keysym.Control-Down:   \033[1;5B
URxvt.keysym.Control-Left:   \033[1;5D
URxvt.keysym.Control-Right:  \033[1;5C

